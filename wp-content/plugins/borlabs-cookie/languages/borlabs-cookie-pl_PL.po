msgid "Active"
msgstr "Aktywny"

#: templates/system-check.html.php:13
msgctxt "Backend / System Check / Text"
msgid "Inactive"
msgstr "Nieaktywny"

#: templates/system-check.html.php:18
msgctxt "Backend / System Check / Table Headline"
msgid "Current Version"
msgstr "Bieżąca wersja"

#: templates/system-check.html.php:22
